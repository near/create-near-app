{
  "name": "near-blank-project",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build": "npm run build:contract && npm run build:web",
    "build:contract": "node contract/compile.js",
    "build:contract:debug": "node contract/compile.js --debug",
    "build:web": "node set-contract-name.js && ng build --prod",
    "dev:deploy:contract": "near dev-deploy",
    "deploy:contract": "near deploy",
    "deploy:pages": "gh-pages -d dist/",
    "deploy": "npm run build && npm run deploy:contract && npm run deploy:pages",
    "prestart": "npm run build:contract && npm run dev:deploy:contract",
    "start": "echo The app is starting! It will automatically open in your browser when ready && node set-contract-name.js && ng serve --open",
    "dev": "nodemon --watch contract -e ts --exec \"npm run start\"",
    "test": "npm run build:contract:debug && cd contract && npm run test && cd .. && ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.11",
    "@angular/common": "~11.2.11",
    "@angular/compiler": "~13.3.1",
    "@angular/core": "~11.2.11",
    "@angular/forms": "~11.2.11",
    "@angular/platform-browser": "~11.2.11",
    "@angular/platform-browser-dynamic": "~11.2.11",
    "@angular/router": "~11.2.11",
    "near-api-js": "~0.41.0",
    "rxjs": "~6.6.3",
    "tslib": "~2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~11.1.1",
    "@angular-devkit/build-angular": "~0.1102.10",
    "@angular/cli": "~11.2.10",
    "@angular/compiler-cli": "~11.2.11",
    "@types/jasmine": "~3.7.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~14.14.10",
    "codelyzer": "~6.0.0",
    "colors": "~1.4.0",
    "dotenv": "~9.0.1",
    "gh-pages": "~3.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "karma-spec-reporter": "~0.0.32",
    "near-cli": "~2.1.1",
    "nodemon": "~2.0.4",
    "protractor": "~7.0.0",
    "puppeteer": "~9.1.1",
    "shelljs": "~0.8.4",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
