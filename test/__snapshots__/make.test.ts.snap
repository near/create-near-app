   const { signedAccountId, loading, signIn, signOut } = useNearWallet();

  const handleAction = () => {
    if (signedAccountId) {
      signOut();
    } else {
      signIn();
    }
  };

  const label = loading
    ? "Loading..."
    : signedAccountId
    ? \`Logout \${signedAccountId}\`
    : "Login";

